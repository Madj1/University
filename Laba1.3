using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;
using System.Threading.Tasks;
using System.Web;

namespace Laba1._3
{
    internal class Program
    {
        static void Main(string[] args)
        {
            double a = 7, b = 5;
            Console.WriteLine("a -> {0}; b -> {1}", a, b);
            Swap(ref a, ref b);
            Console.WriteLine("a -> {0}; b -> {1}", a, b);
            Console.ReadKey();
        }
        public static void Swap(ref double a, ref double b)
        {
            a = a + b;
            b = a - b;
            a = a - b;
        }
    }   
}
